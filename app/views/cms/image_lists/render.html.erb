<div class="image-frame">
    	<div class="container">
    	     <div class="top"><div class="bottom" >
    		<div id="slideShow1" >
			<%
				attachments=ImageList.find_images(@content_block.ImageName1, @content_block.ImageName2, @content_block.ImageName3, @content_block.ImageName4)
				j=0
				for attachment in attachments
				j+=1
			%>
				<div>
					<%if j==1%>
						<a href="<%=@content_block.linkurl1%>"><img src="<%=attachment.file_path%>" alt="" /></a>
						<p> <%= @content_block.description1 %></p>
					<%elsif j==2%>
						<a href="<%=@content_block.linkurl2%>"><img src="<%=attachment.file_path%>" alt="" /></a>
						<p> <%= @content_block.description2 %></p>	
					<%elsif j==3%>
						<a href="<%=@content_block.linkurl3%>"><img src="<%=attachment.file_path%>" alt="" /></a>
						<p> <%= @content_block.description3 %></p>	
					<%elsif j==4%>
						<a href="<%=@content_block.linkurl4%>"><img src="<%=attachment.file_path%>" alt="" /></a>
						<p> <%= @content_block.description4 %></p>	
					<%end%>
				
				</div>
			<%end%>
		</div>
		</div></div>
	    	</div>
    	<div class="relax">&nbsp;</div>
</div>

